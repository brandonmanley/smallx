          SuperscriptBox["x21", "4"], ",", " ", 
          SuperscriptBox["x21", "6"], ",", " ", 
          SuperscriptBox["x21", "8"], ",", " ", 
          SuperscriptBox["x21", "10"], ",", " ", "x21l", ",", " ", 
          RowBox[{"x10", "\[Cross]", "x21"}], ",", " ", 
