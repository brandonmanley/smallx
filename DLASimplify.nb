           RowBox[{"(", " ", 
            RowBox[{"x10", "\[Cross]", "x21"}], ")"}], "2"], ",", " ", "x21k",
           ",", " ", "x21i"}], "}"}]}], ";", "\[IndentingNewLine]", "\t", 
       RowBox[{"powerValues", "=", " ", 
        RowBox[{"{", 
         RowBox[{
         "1", ",", " ", "2", ",", " ", "1", ",", " ", "2", ",", " ", "4", ",",
           " ", "6", ",", " ", "8", ",", "10", ",", " ", "1", ",", " ", "1", 
          ",", " ", "2", ",", " ", "1", ",", " ", "1"}], "}"}]}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", "\t", 
       RowBox[{"num", " ", "=", " ", 
        RowBox[{"Numerator", "[", "exp", "]"}]}], ";", "\[IndentingNewLine]", 
       "\t", 
       RowBox[{"denom", " ", "=", " ", 
        RowBox[{"Denominator", "[", "exp", "]"}]}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", "\t", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"i", "=", "1"}], ",", " ", 
         RowBox[{"i", "<=", " ", 
          RowBox[{"Length", "[", "x21Symbols", "]"}]}], ",", " ", 
         "\[IndentingNewLine]", "\t", 
         RowBox[{
          RowBox[{"j", " ", "=", " ", 
           RowBox[{
            RowBox[{"Count", "[", 
             RowBox[{"num", ",", " ", 
              RowBox[{"x21Symbols", "[", 
               RowBox[{"[", "i", "]"}], "]"}]}], "]"}], " ", "+", " ", 
            RowBox[{"Count", "[", 
             RowBox[{
              RowBox[{"{", "num", "}"}], ",", " ", 
              RowBox[{"x21Symbols", "[", 
               RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}]}], ";", 
          "\[IndentingNewLine]", "\t", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"j", ">", "0"}], ",", " ", 
            RowBox[{"power", "+=", " ", 
             RowBox[{"powerValues", "[", 
              RowBox[{"[", "i", "]"}], "]"}]}]}], "]"}], ";", 
          "\[IndentingNewLine]", "\[IndentingNewLine]", "\t", 
          RowBox[{"k", " ", "=", " ", 
           RowBox[{"Count", "[", 
            RowBox[{
             RowBox[{"{", "denom", "}"}], ",", " ", 
             RowBox[{"x21Symbols", "[", 
