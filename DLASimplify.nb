      RowBox[{
       RowBox[{"rules", " ", "=", " ", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"Cross", "[", 
            RowBox[{"x21", ",", " ", "x21"}], "]"}], "->", "0"}], ",", " ", 
          RowBox[{
