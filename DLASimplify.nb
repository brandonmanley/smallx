              RowBox[{"[", "i", "]"}], "]"}]}]}], "]"}], ";", 
          "\[IndentingNewLine]", "\[IndentingNewLine]", "\t", 
          RowBox[{"i", "+=", "1"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
       "power"}]}], "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"SimplifyTerm", "[", "exp_", "]"}], " ", ":=", " ", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"dlaExp", " ", "=", " ", "0"}], ",", " ", 
